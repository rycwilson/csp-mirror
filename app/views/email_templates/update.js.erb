(function () {

  var status = "<%= @status %>",
      flashMesg = "<%= @flash_mesg %>",
      $submit = $('#email-templates-form').find('[type=submit]');

  flashDisplay(flashMesg, status);

  if (status === 'success') {
    // $("#test-template").prop('disabled', false);
    $submit.prop('disabled', true);
    $('#cancel-template').prop('disabled', true);
  }

})();

