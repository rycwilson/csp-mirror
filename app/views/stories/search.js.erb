
(function () {

  var story_ids = <%= raw @story_ids.to_json %>;

  updateGallery($(
    _.template($('#stories-template').html())({
      stories: app.stories.filter(function (story) { return story_ids.includes(story.id); }),
      isCurator: false
    })
  ));

  $('.search-results').text(story_ids.length.toString() + " stories found").show();

}());