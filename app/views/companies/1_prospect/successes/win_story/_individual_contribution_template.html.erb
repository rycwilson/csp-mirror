
<script type="text/template" id="individual-contribution-template">

  <div class="individual-contribution" data-contribution-id="{{= contributionId }}" data-placeholder="{{= placeholder }}">

    <p>{{= contributor.full_name }}</p>
    <p>{{= contributor.title }}</p>

    <ul>

    {{ theQandA.forEach(function (pair) { }}

      <li>
        <p class="question">{{= pair.question }}</p>
        <p class="answer">{{= pair.answer }}</p>
      </li>

    {{ }); }}

    </ul>

  </div>

</script>