
<script type="text/template" id="group-contribution-template">

  <div class="group-contribution" data-question-id="{{= question.id }}" data-placeholder="{{= placeholder }}">

    <p style="margin-bottom: 0">{{= question.question }}</p>

    <ul>

    {{ answers.forEach(function (answer) { }}

      <li>
        <p style="margin-bottom: 0">{{= answer.contributor.full_name }}</p>
        <p style="margin-bottom: 0; font-style: italic">{{= answer.answer }}</p>
      </li>

    {{ }); }}

    </ul>

  </div></br>

</script>