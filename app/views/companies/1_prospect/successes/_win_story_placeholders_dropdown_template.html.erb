
<script type="text/template" id="win-story-placeholders-dropdown-template">

  {{ var dtContributors = $('#prospect-contributors-table').DataTable(); }}

  <ul class="list-unstyled" style="margin-bottom:0">

    <li data-placeholder="<span contenteditable=&quot;false&quot;>[customer logo]</span>">
      <a href="javascript:;">Customer Logo</a>
    </li>
    <li data-placeholder="<span contenteditable=&quot;false&quot;>[customer description]</span>">
      <a href="javascript:;">Customer Description</a>
    </li>
    <li data-placeholder="<span contenteditable=&quot;false&quot;>[all contributions]</span>">
      <a href="javascript:;">All Contributions</a>
    </li>

    <li style="font-weight: bold; pointer-events: none">Contributor Q&amp;A</li>

    {{ _.uniq(contributionsData.answers, false, function (answer) { return answer.contribution_id; }).forEach(function (answer) { }}

      {{ var contributor = dtContributors.rows('[data-contribution-id="' + answer.contribution_id + '"]').data()[0].contributor; }}

      <li data-placeholder="<div data-contribution-id='{{= answer.contribution_id }}' contenteditable='false'>[Contributor Q&A: {{= contributor.full_name }}]</div>">
        <span>{{= contributor.full_name }}</span>
      </li>

    {{   }); }}

    <li style="font-weight: bold; pointer-events: none">Group Q&amp;A</li>

    {{ contributionsData.questions.forEach(function (question) { }}

      <li data-question-id="{{= question.id }}" data-placeholder="<div contenteditable='false'>[Group Q&A: {{= question.question }}]</div>">
        <span>{{= question.question }}</span>
      </li>

    {{ }) }}

  </ul>

</script>
