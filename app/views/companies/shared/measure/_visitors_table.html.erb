
<table id='measure-visitors-table' width='100%'
       class="table table-striped table-bordered">

  <thead>
    <tr>
      <th>Organization</th>
      <th>Unique visitors</th>
      <th>Visits</th>
    </tr>
  </thead>

  <tbody>

    <% @orgs_visitors.each do |org| %>

      <tr>
        <td><%= org[:name] %></td>
        <td><%= org[:visitors].count %></td>
        <td><%= org[:visits] %></td>
      </tr>

    <% end %>

  </tbody>

</table>
